# This file contains placeholders for production deployment
# Actual values should be set in Vercel's environment variables dashboard

# NextAuth Configuration
NEXTAUTH_URL=https://your-deployment-url.vercel.app
NEXTAUTH_SECRET=nextauth-secret-placeholder

# Authentication
JWT_SECRET=jwt-secret-placeholder
JWT_EXPIRES_IN=24h

# Email Service (Resend.com)
RESEND_API_KEY=resend-api-key-placeholder
EMAIL_FROM=noreply@yourdomain.com

# API URLs
NEXT_PUBLIC_API_URL=
BACKEND_API_URL=

# MongoDB URI
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/motion-magic

# Admin Configuration
ADMIN_API_KEY=admin-api-key-placeholder
ADMIN_INITIAL_PASSWORD=admin-password-placeholder

# Public Environment Variables
NEXT_PUBLIC_APP_URL=https://your-deployment-url.vercel.app 